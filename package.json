{
  "name": "next-tpl",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "qualityCheck": "run-s qualityCheck:*",
    "qualityCheck:type": "tsc --project ./tsconfig.json --noEmit",
    "qualityCheck:unitTest": "jest --testPathIgnorePatterns .gitignore",
    "qualityCheck:redundancy": "jscpd .",
    "qualityCheck:findDeadCode": "ts-prune -e",
    "qualityCheck:esLint": "eslint --ignore-path .gitignore .",
    "qualityCheck:fmt": "prettier  --check --ignore-path .gitignore .",
    "qualityCheck:stylesLint": "stylelint --fix --ignore-path .gitignore '**/styles.ts'",
    "prepare": "git config --local core.hooksPath .githooks"
  },
  "dependencies": {
    "next": "12.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@semantic-release/git": "10.0.1",
    "@stylelint/postcss-css-in-js": "0.37.2",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.0.2",
    "@types/react": "17.0.33",
    "@types/styled-components": "5.1.15",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "babel-plugin-jsx-remove-data-test-id": "3.0.0",
    "conventional-changelog-conventionalcommits": "4.6.1",
    "eslint": "8.1.0",
    "eslint-config-next": "12.0.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jest": "25.2.2",
    "interactive-commit": "2.0.5",
    "jest": "27.3.1",
    "jscpd": "3.3.26",
    "npm-run-all": "4.1.5",
    "postcss-syntax": "0.36.2",
    "prettier": "2.4.1",
    "semantic-release": "18.0.0",
    "stylelint": "14.0.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recess-order": "2.5.0",
    "stylelint-config-standard": "23.0.0",
    "ts-jest": "27.0.7",
    "ts-prune": "0.10.1",
    "typescript": "4.4.4"
  }
}
